{
   gomp2
   Memcheck:Leak
   match-leak-kinds: reachable
   fun:malloc
   fun:gomp_malloc
   fun:gomp_init_num_threads
   fun:initialize_env
   fun:call_init.part.0
   fun:call_init
   fun:_dl_init
   ...
}

